<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@android:color/white"
    android:padding="8dp"
    tools:layout_editor_absoluteY="89dp">

    <View
        android:id="@+id/view3"
        android:layout_width="1dp"
        android:layout_height="100dp"
        android:layout_marginLeft="24dp"
        android:layout_marginStart="24dp"
        android:background="@color/gary"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/txt_status_info"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/txt_status_info"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:gravity="center_vertical|center_horizontal"
        android:stateListAnimator="@null"
        android:textColor="#fff"
        android:textSize="14sp"
        app:layout_constraintEnd_toStartOf="@+id/view3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txt_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        android:text="เวลา : "
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/view3"
        app:layout_constraintTop_toBottomOf="@+id/txt_sta_view"
        app:layout_constraintVertical_bias="0.0" />


    <TextView
        android:id="@+id/txtResult"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        app:layout_constraintBaseline_toBaselineOf="@+id/txt_time"
        app:layout_constraintStart_toEndOf="@+id/txt_time" />


    <TextView
        android:id="@+id/txt_appr_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        android:text="โดย  : "
        app:layout_constraintBaseline_toBaselineOf="@+id/txtResult"
        app:layout_constraintStart_toEndOf="@+id/txtResult" />

    <TextView
        android:id="@+id/appr_name"

        android:layout_width="wrap_content"
        android:layout_height="18dp"
        android:textColor="#000"
        app:layout_constraintBaseline_toBaselineOf="@+id/txt_appr_name"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/txt_appr_name"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/txt_gone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="172dp"
        android:layout_marginStart="172dp"
        android:layout_marginTop="8dp"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text_4_desc" />

    <TextView
        android:id="@+id/text_1_desc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:text="TextView"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/txt_time"
        app:layout_constraintTop_toBottomOf="@+id/txt_time"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/text_2_desc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:text="TextView"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/txt_time"
        app:layout_constraintTop_toBottomOf="@+id/text_1_desc"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/text_3_desc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:text="TextView"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/txt_time"
        app:layout_constraintTop_toBottomOf="@+id/text_2_desc"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/text_4_desc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:text="TextView"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/txt_time"
        app:layout_constraintTop_toBottomOf="@+id/text_3_desc"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/text_desc1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:text="TextView"
        android:textColor="#0000FF	"
        android:visibility="gone"
        app:layout_constraintBaseline_toBaselineOf="@+id/text_1_desc"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/text_1_desc" />

    <TextView
        android:id="@+id/text_desc2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textColor="#0000FF	"
        android:visibility="gone"
        app:layout_constraintBaseline_toBaselineOf="@+id/text_2_desc"
        app:layout_constraintStart_toEndOf="@+id/text_2_desc" />

    <TextView
        android:id="@+id/text_desc3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textColor="#0000FF	"
        android:visibility="gone"
        app:layout_constraintBaseline_toBaselineOf="@+id/text_3_desc"
        app:layout_constraintStart_toEndOf="@+id/text_3_desc" />

    <TextView
        android:id="@+id/text_desc4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textColor="#0000FF	"
        android:visibility="gone"
        app:layout_constraintBaseline_toBaselineOf="@+id/text_4_desc"
        app:layout_constraintStart_toEndOf="@+id/text_4_desc" />

    <TextView
        android:id="@+id/txt_view_after"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:text="TextView"
        android:textColor="#FF0000"
        android:visibility="gone"
        app:layout_constraintBaseline_toBaselineOf="@+id/text_desc1"
        app:layout_constraintStart_toEndOf="@+id/txt_alt26" />

    <TextView
        android:id="@+id/txt_alt26"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:text="→"
        android:textStyle="bold"
        android:visibility="gone"
        app:layout_constraintBaseline_toBaselineOf="@+id/text_desc1"
        app:layout_constraintStart_toEndOf="@+id/text_desc1" />

    <TextView
        android:id="@+id/txt_sta_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:textColor="#000"
        app:layout_constraintStart_toEndOf="@+id/view3"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txt_desc_"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="gone"
        app:layout_constraintStart_toStartOf="@+id/txt_time"
        app:layout_constraintTop_toBottomOf="@+id/txt_time" />

    <TextView
        android:id="@+id/txt_wait_re"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:text="TextView"
        android:visibility="gone"
        app:layout_constraintBaseline_toBaselineOf="@+id/text_1_desc"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/text_desc1" />


</android.support.constraint.ConstraintLayout>
